{
  "version": 2,
  "builds": [
    { "src": "FH_FrontEnd/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "FH_BackEnd/api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "FH_BackEnd/api/$1.js" },
    { "src": "/(.*)", "dest": "FH_FrontEnd/dist/$1" }
  ]
}
